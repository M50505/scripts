#_doc ${1} 20250302100238 " " && exit 0

cdproj ()
{
	cd ~/projects/${1}
	
	if [[ -d .git ]]
	then
		local user=$(git config --get remote.origin.url)
		user=${user#*:}
		user=${user%/*}
		gitInit ${user}
	fi
}
