wlSetDomainCredentials ()
{
	_doc $1 20140530182202 "wlSetDomainCredentials accepts the currenlty used administrative username and password to create the boot.properties and NodeManager user key and user configuration files." && return 0

	local result=""
	local status=1

	if [[ $# -gt 0 ]]
	then

		result="New function wlSetDomainCredentials"
		status=0
	fi

	echo "${result}"

	return ${status}
}

