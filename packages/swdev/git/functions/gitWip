_doc ${1} 20211118224002 "Not documented yet." && exit 0

gitWip ()
{
	if [[ $# -gt 0 ]]
	then
		message="${*}"
	else
		message="WIP"
	fi

	if gitIsUpdated
	then
		echo gitIsUpdate passes, now using gitCdRoot and then gitPub
		(
			gitCdRoot
			gitPub "${message}"
		)	
	fi

}
