gitcred ()
{
	if [[ $# -gt 0 ]]
	then
		rm -f ${HOME}/.git-credentials ${HOME}/.gitconfig

		cp /local/gitcred-${1} ${HOME}/.git-credentials
		cp /local/gitconfig-${1} ${HOME}/.gitconfig
	else
		cat ${HOME}/.git-credentials
		echo ------------------------------------
		cat ${HOME}/.gitconfig
	fi



	#git credential-cache exit
}

