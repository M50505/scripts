gitIsUpdated ()
{
	_doc ${1} 20211118223101 "Not focumented yet. " && return 0
	local result
	((result=$(git status --short | wc -l)))

	if [[ ${result} -eq 0 ]]
	then
		result=1
	else
		result=0
	fi

	return ${result}
}
